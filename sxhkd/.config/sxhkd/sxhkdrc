# Reload
super + Home
    reload-wm

# Terminal
super + Return
    st

# Launcher
super + a
    rofi -show drun -display-drun "run:"

# Browser
super + b
    firefox

# File manager
super + z
    st -e ranger

# Toggle floating mode
super + shift + f
    bspc node -t \~floating

# Close window
super + q
    bspc node -c

# Kill window
super + k
    bspc node -k

